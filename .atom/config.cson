"*":
  "command-palette":
    preserveLastSearch: true
  core:
    audioBeep: false
    closeDeletedFileTabs: true
    closeEmptyWindows: false
    disabledPackages: [
      "find-and-replace-under-cursor"
      "highlight-selected"
      "tidy-tabs"
      "parcel"
      "minimap"
      "minimap-autohider"
      "minimap-bookmarks"
      "minimap-cursorline"
      "minimap-highlight-selected"
      "minimap-quick-highlight"
      "symbol-gen"
      "symbols-tree-view"
      "teletype"
      "todo-show"
      "ide-sourcekit"
      "ide-swift"
      "atom-ide-ui"
      "data-atom"
      "linter-swiftlint"
      "linter-ui-default"
    ]
    excludeVcsIgnoredPaths: false
    openEmptyEditorOnStart: false
    packagesWithKeymapsDisabled: [
      "dash"
    ]
    telemetryConsent: "limited"
    themes: [
      "atom-dark-ui"
      "atom-dark-syntax"
    ]
  editor:
    atomicSoftTabs: false
    invisibles: {}
    preferredLineLength: 100
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
    tabLength: 4
    tabType: "soft"
  "exception-reporting":
    userId: "6b982d8d-d8c9-4aa6-adc2-6a5052b248bc"
  "find-and-replace":
    closeFindPanelAfterSearch: true
  "fuzzy-finder":
    preserveLastSearch: true
    searchAllPanes: true
  "git-diff":
    showIconsInEditorGutter: true
  github: {}
  "highlight-selected":
    hideHighlightOnSelectedWord: true
    highlightBackground: true
    lightTheme: true
  "linter-ui-default":
    panelHeight: 300
  minimap:
    absoluteMode: true
    adjustAbsoluteModeHeight: true
    plugins:
      bookmarks: true
      bookmarksDecorationsZIndex: 0
      cursorline: true
      cursorlineDecorationsZIndex: 0
      "highlight-selected": true
      "highlight-selectedDecorationsZIndex": 0
      "minimap-autohider": true
      "minimap-autohiderDecorationsZIndex": 0
      "minimap-quick-highlight": true
      "minimap-quick-highlightDecorationsZIndex": 0
    scrollAnimation: true
  "minimap-autohider":
    TimeToHide: 5000
  "quick-highlight": {}
  tabs:
    enableVcsColoring: true
  "todo-show":
    sortBy: "Path"
  "tree-view": {}
  "vim-mode-plus":
    keymapYToYankToLastCharacterOfLine: true
    useClipboardAsDefaultRegister: false
    useSmartcaseForSearch: true
  welcome:
    showOnStartup: false
  whitespace:
    ignoreWhitespaceOnCurrentLine: false
".source.swift":
  editor:
    autoIndent: true
